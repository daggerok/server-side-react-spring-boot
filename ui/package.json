{
  "name": "server-side-react-spring-boot",
  "version": "0.0.1",
  "description": "server-side-react-spring-boot",
  "main": "./src/main.js",
  "scripts": {
    "i": "npm i --no-optional",
    "prebuild": "npm run i",
    "build:clean": "rimraf -rf ../src/main/resources/static/**/*.{js,html}",
    "build:compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.babel.js -c --process -p",
    "build": "npm-run-all build:*",
    "prestart": "npm run i",
    "start": "webpack-dev-server -d --config ./config/webpack.config.babel.js --content-base=../src/main/resources/static",
    "serve": "json-server ../src/main/resources/static/db.json -d 500 -w -p 3000 --static ../src/main/resources/static",
    "webpack:watch": "webpack -w --config ./config/webpack.config.babel.js -c --process",
    "webpack:compile": "webpack --config ./config/webpack.config.babel.js -c --process"
  },
  "repository": {
    "type": "git",
    "url": "https://githuib.com/daggerok/server-side-react-spring-boot"
  },
  "keywords": [
    "react"
  ],
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.24.0",
    "cross-env": "3.2.4",
    "css-loader": "0.27.3",
    "extract-text-webpack-plugin": "<2",
    "file-loader": "0.10.1",
    "handlebars": "4.0.6",
    "handlebars-loader": "1.4.0",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "4.0.2",
    "npm-run-all": "4.1.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "1.3.3",
    "rimraf": "2.6.1",
    "source-map-loader": "0.2.0",
    "style-loader": "0.16.0",
    "url-loader": "0.5.8",
    "webpack": "<2",
    "webpack-dev-server": "<2"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "bootstrap": "3.3.7",
    "jquery": "3.2.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "<4.0.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "transform-class-properties"
    ]
  }
}
